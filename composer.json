{
    "name": "equipc/laravel-email-validator",
    "description": "Validation rule to validate email for Laravel",
    "homepage": "https://github.com/EquiPC/laravel-email-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel LAURENT",
            "email": "michel.laurent@equipc.net"
        }
    ],
    "require": {
        "php" : "^7.3",
        "illuminate/support": "^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "EquiPC\\EmailValidator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EquiPC\\EmailValidator\\EmailValidatorServiceProvider"
            ],
            "aliases": {
                "EmailValidator": "EquiPC\\EmailValidator\\EmailValidatorFacade"
            }
        }
    }
}
